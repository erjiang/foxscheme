{
  "name": "foxscheme",
  "version": "0.2.0",
  "description": "A Scheme interpreter written in JavaScript.",
  "main": "fox.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "webpack",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "build:grammar": "peggy --format commonjs -o src/system/parser.generated.js grammar.peg",
    "repl": "node -r ts-node/register src/repl.ts",
    "test": "mocha -r ts-node/register test/interpreter.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erjiang/foxscheme.git"
  },
  "author": "Eric Jiang <erjiang@indiana.edu>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/erjiang/foxscheme/issues"
  },
  "homepage": "https://github.com/erjiang/foxscheme#readme",
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/compat-data": "^7.27.3",
    "@babel/core": "^7.27.3",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.8.6",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.7",
    "babel-loader": "^8.0.5",
    "mocha": "^11.5.0",
    "source-map-loader": "^5.0.0",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.4",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "peggy": "^5.0.2"
  }
}
